	TG_OUT[NG_Intro].CR_vidange_Dry[liste_Seq_PPE[4]].en_cours := FALSE;
	TG_OUT[NG_Intro].CR_vidange_Dry[liste_Seq_PPE[4]].termine := TRUE;
END_IF;

RE_adj5(CLK:=Seq_ppe_vers_malax_termine[5]);
