		ST cumuls_pesees.Cumuls_pesees_Gra[ma0].cumul_poids_reellement_peses
		JMP init_corr_g_g_gra2__boucle
	init_corr_g_g_gra2__fin:


 	(* Rcupration des options de dosage *)

