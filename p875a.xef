RAZ_Alarmes := Pup_Cde_RAZ_alarmes OR (Info_RAZ_Alarmes AND Acquisition_entrees_ACTIVE);

TON_1_RAZ_Alarmes(IN := RAZ_Alarmes, PT := t#2000ms);
(* La tempo incite le centraliste  ne pas prendre l'habitude *)
(* d'appuyer systmatiquement sur le bouton et  analyser d'avantage *)
(* la cause du problme. Elle permet galement de ne pas prendre en *)
(* compte un appui par erreur sur le mauvais bouton. *)

TON_2_RAZ_Alarmes(IN := RAZ_Alarmes, PT := t#15s);
(* Cette tempo "longue" est prvue pour ne pas reprendre *)
(* la production en cas d'arrt d'urgence comme on le ferait pour *)
(* un autre dfaut (par exemple "Alarme_Controle_Tension"). *)
