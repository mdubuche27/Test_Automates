(* Skip bloqu *)

(* On surveille le temps mis par le skip entre les capteurs haut, bas, et attente *)
CAL fm_montee_skip (CLK := Cde_montee_skip)
CAL fm_descente_skip (CLK := Cde_descente_skip)

LD fm_montee_skip.Q
OR fm_descente_skip.Q
