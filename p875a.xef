	ST NP_Gra2
	LD FALSE
   	ST Dosage_auto_Gra2
  	
 fin_granulat2:
 (*Dfaut de pese*)
 ld Pesee_bloquee_Gra2
(* or Alarme_poids_max_granulat2*)(* On ne bloque plus la validation de la pese *)
