(* Cette section de communication utilise un EFB de communication
   spcifique au M340 *)
(* La passerelle HMS est utilise en ROUMANIE *)




(*----------------- GESTION DES TRAMES  ------------------------------------*)
(*
En fonctionnement normal, on effectue une lecture/criture toute les 20ms (mise  jour Siwarex).
En cas d'erreur ou de Time Out (100ms), Il on remmet 3 fois la trame. 
Au bout des 3 fois, on active le dfaut communication.
Le processus n'est pas des plus simple mais il permet de dtecter rapidement un defaut communication.

Une methode plus simple en codage serait de considrer le time out com comme dfaut communication. mais dans ce cas, le time out doit 
accepter le plus long des temps de rponse ... mesur  environ 700ms sur la centrale de Stefanisti Roumanie, ce qui est plutot long en cours de dosage!
*)

(* La longeur des donnes transmise est calcule sur la taile des tableau
   a envoyer et  receptionner 
   Equation de correspondance profibus *)
(* NETTOOL : Octet en lecture = 60
   + 8 Octet de live list = 68
   / 2 = 34 mots
   + 1 (car le premier octet est l'octet 0) = @35 dans la HMS *)
(* NETTOOL : Octet en Ecriture = 60
   / 2 = 30 mots
   + 1 (car le premier octet est l'octet 0) = @31 dans la HMS
   --&gt; les ecritures sont dcale  l'adresse 1025 dans la HMS *)

(* Initialisation des variables locales de la section *)
IF Com_ETH_HMS_Profibus_DEM_INIT THEN

(*
	(Mettre les futures initialisations ici)
*)

	(* Reset de la demande d'init *)
	Com_ETH_HMS_Profibus_DEM_INIT := FALSE; (* L'init de la section a bien t prise en compte. *)
END_IF;

IP_HMS (IP1 := Conf_HMS_Profibus_IP1,
	IP2 := Conf_HMS_Profibus_IP2,
	IP3 := Conf_HMS_Profibus_IP3,
	IP4 := Conf_HMS_Profibus_IP4,
	Adresse =&gt; Adresse_HMS);

Com_ETH_HMS (TimeOut := 1,
		BDT_Com := t#20ms,
		Cible_TCP := Adresse_HMS,
		Tableau_envoi := Valeurs_envoi_TR1,
		Adresse_Lecture := 0,
		Adresse_Ecriture := 1024,
		Tableau_Reception =&gt; Valeurs_recu_TR1,
		Alarme_Com_API_ModbusTCP_1 =&gt; Def_com_HMS,
		Diag_Lecture =&gt; Rapport_Lec,
		Diag_Ecriture =&gt; Rapport_Ecr);
</STSource>
