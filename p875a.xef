		LE 4 (* d'un camion/malaxeur, *)
	) (* alors l'introduction doit attendre la fin du dosage correspondant. *)
	(* Note R.C. 10/04/2012 : Il n'y a pas de cas actuellement o on utilise la *)
	(* zone "Compteurs" pour faire des dosages sans tampon vers le camion/malaxeur. *)
	(*    ce cas (initialement prvu) se fait tout comptes faits par la *)
	(*    zone "Dos_dif_dry". =&gt; Consquence : Actuellement, tous les dosages non *)
	(*    termins de la zone "Compteurs" bloquent le lancement de l'introduction. *)
	ANDN Dosages_ppe_termine_Section300[idx_ppe] (* Si le dosage 'i' demand (dans *)
		(* la zone "Compteurs" de la TG) n'est pas termin pour la pompe 'idx_ppe' *)
		(* correspondante, alors on dclare que l'tat des "dosages par pompe" *)
		(* doit bloquer l'introduction. *)
	ANDN Dosages_Ppe_termines (* Si tous les dosages par pompe de la gche en *)
		(* cours d'introduction sont termins (pour toutes les pompes), alors *)
		(* on ne tient pas compte du fait que le dosage ne soit pas vu *)
		(* "termin" pour la pompe 'idx_ppe'. *)
		(* Effectivement, le statut "termin" peut repasser  FALSE pour la *)
		(* pompe lorsque la section de dosage par pompe passe  la gche *)
		(* suivante. *)
		(* Explications supplmentaire R.C. 03/04/2012 : "Dosages_Ppe_termines" *)
		(* est  TRUE quand tous les dosages par pompe par la zone "Compteurs" *)
		(* de la gche en cours d'introduction sont termins. *)
		(* Si "Dosages_Ppe_termines" est  TRUE, alors il ne faut pas faire *)
		(* de SET de "Dosage_Ppe_bloquant_pour_intro" quel que soit le statut *)
		(* individuel de la pompe (pour les raisons voques ci-dessus). *)
	S Dosage_Ppe_bloquant_pour_intro[i]
	JMP Test_dosages_ppe_bloquants
Test_dosages_ppe_bloquants__fin:
(* - On bloque l'introduction si on trouve 1 lment  TRUE dans "Dosage_Ppe_bloquant_pour_intro[]" *)
LD Dosage_Ppe_bloquant_pour_intro
MOVE_AREBOOL_INT Liste_Dosages_Ppe_bloquants
LD Liste_Dosages_Ppe_bloquants
NE 0 (* Un des dosages de la zone "Compteurs" de la TG bloque l'introduction *)
JMPC Fin_Section_390


(* Test equipement centrale pour une anticipation d'introduction *)
LD destination_malaxeur_1
AND (	Conf_granulat1_skip
	OR Conf_skip_transfert
)
JMPCN fin_anticipation

(* Vrification granulat  introduire *)
LD TG_IN[suite_index_introduction].Vidange_Dry[1].Code_prod
AND 16#00FF
NE 1
JMPC fin_anticipation

	(* Si temps restant &lt; temps de montee du  skip jusqu' position intermediaire *)
	LDN malaxeur_1_vide
	AND (	TRUE
		AND (	Conf_skip_transfert
			ANDN Conf_TT_granulat
			AND Transfert_G_terminee
			ANDN Ordre_intro_Gra1
			ANDN Ordre_intro_Gra2
		)
		OR (	Conf_granulat1_skip
			AND Pesee_Gra1_terminee
			ANDN Ordre_intro_Gra1
		)
	)
	AND anticipation_skip
	ANDN Introduction_Gra_terminee
	S anticipation_montee_skip

fin_anticipation: 


(* Dbut d'introduction si le malaxeur concern est vide, *)
(*  en marche et que ces trappes sont bien fermes *)
LD Camion_OK
S Camion_OK_Dosages_OK (* Modif R.C. 05/09/2003 *)
	(* Permet le lancement de l'alarme malaxeur arrt *)
AND Camion_OK_Dosages_OK_Malaxeur_OK (* Modif R.C. 05/09/2003 *)
S Intro_en_cours
 
(*
ANDN Bit_Gachee_dry  		 
S Pas_Vidange_Dry (* Modif R.C. 22/06/2009 *)  
*)(* Modif. M.D. xx/01/2011 on veut lancer la vidange Dry dans tous les cas *)

LD Intro_en_cours		
JMPCN Fin_Section_390

	(*Dclaration de malaxeur plein*) 
	LD destination_malaxeur_1
	R declaration_malaxeur_1_vide
	LD destination_malaxeur_2
	R declaration_malaxeur_2_vide

Fin_Section_390 :
