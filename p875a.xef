		<attribute name="TypeCodeCheckSumString" value="653E"></attribute>
		<attribute name="TypeSignatureCheckSumString" value="C9A7"></attribute>
		<inputParameters>
			<variables name="Dem_seq_ouv" typeName="BOOL">
