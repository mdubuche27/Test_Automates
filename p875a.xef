		IndexSeqVid_Tampon_ppe[Idx_Ppe] := 0; (* Initialisation non obligatoire, faite pour viter a puisse perturber le dbug *)
		Nb_intros_tampons_ppe_restantes := Nb_intros_tampons_ppe_restantes - 1;
