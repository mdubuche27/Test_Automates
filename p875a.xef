	<program>
		<identProgram name="480_Commandes_pupitre" type="section" task="MAST"></identProgram>
		<STSource>(*
MD 14/
!!!!! "cde_dosage_cim" sont crites dans diffrentes sections (suivant le mode AUTO, MANU ...)
cette criture n'est pas propre, il faudrait fusionner ces diffrentes critures dans une seule section
*)


(* Initialisation des variables locales de la section *)
IF Commandes_pupitre_DEM_INIT THEN

(*
	(Mettre les futures initialisations ici)
*)

	(* Reset de la demande d'init *)
	Commandes_pupitre_DEM_INIT := FALSE; (* L'init de la section a bien t prise en compte. *)
END_IF;


(* Modif Cim2 M.D. 26/03/09 Ajout Bascule Ciment 2 *)
(* Pour chaque commande de dosage, il faut vrifier la destination du silo!!! *)
IF NOT Info_Plant_In_Auto THEN
	(* Ajout des commandes pupitre de dosage *)
	(* Dans le programme les autres commandes sont indexes. *)
	(* On cre ici les commandes pupitre qui seront utilises pour crire les sorties*)


	(********************** GRANULAT 1 &amp; GRANULAT 2 ****************************)

	(* "Numero_manu_pup_*" contient le numero de l'organe de stockage surlequel *)
	(* un dosage est autoris. On n'autorise qu'un dosage par bascule pour pouvoir *)
	(* dterminer les consommations par organe de stockage. *)
	(* On n'autorise le changement d'organe ou la vidange de la bascule qu'une fois *)
	(* le cumul fait. Dans ce but, on passe "Numero_manu_pup_*"  0 pour savoir que *)
	(* le cumul est fait. *)
	FOR idx_P1:=1 TO (Conf_granulat1_nb_tremies + Conf_granulat2_nb_tremies) BY 1 DO
		IF (Pup_Cde_dos_Gra[idx_P1] = TRUE) THEN (* Demande de dosage par le pupitre *)
			IF (idx_P1 &lt;= Conf_granulat1_nb_tremies) THEN (* Ligne Gra1 *)
				IF (Numero_manu_pup_Gra1 = 0) (* Basc Gra1 dispo pour un dosage (le cumul conso a t mis  jour aprs le dosage prcdent) *)
				AND TON_fin_vidange_dans_Gra1.Q (*    Pas de bascule en cours de vidange vers lorgane de pesage Gra1 *)
				AND (Numero_manu_pup_Gra2 = 0) THEN (* Ncessaire car la section grant les cumuls ne sait pas traiter des dosages simultans sur les 2 lignes *)
					Numero_manu_pup_Gra1 := idx_P1;
				END_IF;
			ELSE (* Ligne Gra2 *)
				IF (Numero_manu_pup_Gra2 = 0) (* Basc Gra2 dispo pour un dosage (le cumul conso a t mis  jour aprs le dosage prcdent) *)
				AND TON_fin_vidange_dans_Gra2.Q (*    Pas de bascule en cours de vidange vers lorgane de pesage Gra2 *)
				AND (Numero_manu_pup_Gra1 = 0) THEN (* Ncessaire car la section grant les cumuls ne sait pas traiter des dosages simultans sur les 2 lignes *)
					Numero_manu_pup_Gra2 := idx_P1;
				END_IF;
			END_IF;
		END_IF;
	END_FOR;

	(* FAIRE LE DOSAGE *)
	FOR Idx_P1:=1 TO 14 BY 1 DO
		Cde_dosage_Gra[Idx_P1] := 
			NOT Info_Plant_In_Auto
			AND Pup_Cde_dos_Gra[Idx_P1]
			AND ((
				(Numero_manu_pup_Gra1 = idx_P1)
				AND (Info_balance_Gra1_fermee OR (Conf_Granulat1_tapis OR Conf_Granulat1_skip))
				AND (NOT Cde_vidange_bascule_Gra1 OR NOT Conf_Granulat1_tapis)
				AND (Cable_skip_mou OR NOT Conf_granulat1_skip)
				AND NOT Alarme_poids_max_Gra1
				AND NOT Alarme_Com_HMS_Profibus_Gra1
			) OR (
				(Numero_manu_pup_Gra2 = idx_P1)
				AND (Info_balance_Gra2_fermee OR NOT Conf_granulat2_bascule)
				AND (NOT Cde_vidange_bascule_Gra2 OR NOT Conf_Granulat2_tapis)
				AND NOT Alarme_poids_max_Gra2
				(*AND NOT Alarme_Com_HMS_Profibus_Gra2*) (*Pas encore implment*)
			))
			AND NOT Alarme_Com_API_ModbusTCP_1 (* TOUTES BASCULES SUR HMS ... *)
			AND NOT Alarme_Controle_Tension;

(*
			AND (FALSE
				OR (Conf_granulat1_skip
					AND Cable_skip_mou)
				OR (Conf_granulat1_tapis
					AND NOT Cde_vidange_bascule_granulat1
					AND ( Conf_granulat1_pres_capteur_rot
						AND tempo_ctrle_rotation_G1.Q
						OR NOT Conf_granulat1_pres_capteur_rot)
				)
				OR (NOT Conf_granulat1_skip
					AND NOT Conf_granulat1_tapis
					AND Info_balance_granulat1_fermee)
			);
 *)
	END_FOR;


	(********************** CIMENT 1 &amp; CIMENT 2 ****************************)

	(* "Numero_manu_pup_*" contient le numero de l'organe de stockage surlequel *)
	(* un dosage est autoris. On n'autorise qu'un dosage par bascule pour pouvoir *)
	(* dterminer les consommations par organe de stockage. *)
	(* On n'autorise le changement d'organe ou la vidange de la bascule qu'une fois *)
	(* le cumul fait. Dans ce but, on passe "Numero_manu_pup_*"  0 pour savoir que *)
	(* le cumul est fait. *)
	FOR idx_P1:=1 TO (Conf_Cim1_nb_silos + Conf_Cim2_nb_silos) BY 1 DO
		IF (Pup_Cde_dos_Cim[idx_P1] = TRUE) THEN (* Demande de dosage par le pupitre *)
			IF (idx_P1 &lt;= Conf_Cim1_nb_silos) THEN (* Ligne Cim1 *)
				IF (Numero_manu_pup_Cim1 = 0) THEN (* Basc Cim1 dispo pour un dosage (le cumul conso a t mis  jour aprs le dosage prcdent) *)
					Numero_manu_pup_Cim1 := idx_P1;
				END_IF;
			ELSE (* Ligne Cim2 *)
				IF (Numero_manu_pup_Cim2 = 0) THEN (* Basc Cim2 dispo pour un dosage (le cumul conso a t mis  jour aprs le dosage prcdent) *)
					Numero_manu_pup_Cim2 := idx_P1;
				END_IF;
			END_IF;
		END_IF;
	END_FOR;

	(* FAIRE LE DOSAGE *)
	FOR Idx_P1:=1 TO 6 BY 1 DO
		Cde_dosage_Cim[Idx_P1] := 
			NOT Info_Plant_In_Auto
			AND Pup_Cde_dos_Cim[Idx_P1]
			AND ((
				(Numero_manu_pup_Cim1 = idx_P1)
				AND Info_balance_Cim1_fermee
				AND NOT Alarme_poids_max_Cim1
				AND NOT Alarme_Com_HMS_Profibus_Cim1
			) OR (
				(Numero_manu_pup_Cim2 = idx_P1)
				AND NOT Alarme_poids_max_Cim2
				AND Info_balance_Cim2_fermee
				(*AND NOT Alarme_Com_HMS_Profibus_Cim2*) (*Pas encore implment*)
			))
			AND NOT Alarme_Com_API_ModbusTCP_1 (* TOUTES BASCULES SUR HMS ... *)
			AND NOT Alarme_Controle_Tension;
	END_FOR;


	(********************** EAU 1 &amp; EAU 2 ****************************)

	(* "Numero_manu_pup_*" contient le numero de l'organe de stockage surlequel *)
	(* un dosage est autoris. On n'autorise qu'un dosage par bascule pour pouvoir *)
	(* dterminer les consommations par organe de stockage. *)
	(* On n'autorise le changement d'organe ou la vidange de la bascule qu'une fois *)
	(* le cumul fait. Dans ce but, on passe "Numero_manu_pup_*"  0 pour savoir que *)
	(* le cumul est fait. *)
	FOR idx_P1:=1 TO 4 BY 1 DO (* Rappel : le nombre de cuves  eau utilises ne peut tre dtermin d'aprs "Conf_eau1_nb_cuves" et "Conf_eau2_nb_cuves" *)
		IF (Pup_Cde_dos_Eau[idx_P1] = TRUE) THEN (* Demande de dosage par le pupitre *)
			IF (Conf_Eau_dest_cuves[Idx_P1] = 0) THEN (* Cuve Eau pese *)
				IF (idx_P1 &lt;= Conf_Eau1_nb_cuves) THEN (* Ligne Eau1 *)
					IF (Numero_manu_pup_Eau1 = 0) (* Basc Eau1 dispo pour un dosage (le cumul conso a t mis  jour aprs le dosage prcdent) *)
					AND TON_fin_dosage_ppe_vers_Eau1.Q (*    Pas de pompe en cours de dosage vers la bascule Eau1 *)
					AND TON_fin_vidange_dans_Eau1.Q THEN (*    Pas de bascule en cours de vidange dans la bascule Eau1 *)
						Numero_manu_pup_Eau1 := idx_P1;
					END_IF;
				ELSE (* Ligne Eau2 *)
					IF (Numero_manu_pup_Eau2 = 0) THEN (* Basc Eau2 dispo pour un dosage (le cumul conso a t mis  jour aprs le dosage prcdent) *)
						Numero_manu_pup_Eau2 := idx_P1;
					END_IF;
				END_IF;
			END_IF;
		END_IF;
	END_FOR;

	(* FAIRE LE DOSAGE *)
	FOR Idx_P1:=1 TO 4 BY 1 DO
		Cde_dosage_Eau[Idx_P1] := 
			NOT Info_Plant_In_Auto
			AND Pup_Cde_dos_Eau[Idx_P1]
			AND ((
				(Conf_Eau_dest_cuves[Idx_P1] = 0) (* Cuve Eau pese *)
				AND (Numero_manu_pup_Eau1 = idx_P1) (* Eau1 *)
				AND Info_balance_Eau1_fermee
				AND NOT Alarme_poids_max_Eau1
				AND NOT Alarme_Com_HMS_Profibus_Eau1
			) OR (
				(Conf_Eau_dest_cuves[Idx_P1] = 0) (* Cuve Eau pese *)
				AND (Numero_manu_pup_Eau2 = idx_P1) (* Eau2 *)
				AND Info_balance_Eau2_fermee
				AND NOT Alarme_poids_max_Eau2
				(*AND NOT Alarme_Com_HMS_Profibus_Eau2*) (*Pas encore implment*)
			) OR (
				(Conf_Eau_dest_cuves[Idx_P1] &lt;&gt; 0) (* Cuve Eau compte *)
			))
			AND NOT Alarme_Com_API_ModbusTCP_1 (* TOUTES BASCULES SUR HMS ... *)
			AND NOT Alarme_Controle_Tension;
	END_FOR;


	(********************** ADJUVANT ****************************)

	(* "Numero_manu_pup_*" contient le numero de l'organe de stockage surlequel *)
	(* un dosage est autoris. On n'autorise qu'un dosage par bascule pour pouvoir *)
	(* dterminer les consommations par organe de stockage. *)
	(* On n'autorise le changement d'organe ou la vidange de la bascule qu'une fois *)
	(* le cumul fait. Dans ce but, on passe "Numero_manu_pup_*"  0 pour savoir que *)
	(* le cumul est fait. *)
	FOR idx_P1:=1 TO Conf_Adj1_nb_cuves BY 1 DO
		IF (Pup_Cde_dos_Adj[idx_P1] = TRUE) THEN (* Demande de dosage par le pupitre *)
			IF (Conf_Adj_dest_cuves[Idx_P1] = 0) THEN (* Cuve Adj pese *)
				IF (Numero_manu_pup_Adj = 0) THEN (* Basc Adj dispo pour un dosage (le cumul conso a t mis  jour aprs le dosage prcdent) *)
					Numero_manu_pup_Adj := idx_P1;
				END_IF;
			END_IF;
		END_IF;
	END_FOR;

	(* FAIRE LE DOSAGE *)
	FOR Idx_P1:=1 TO 20 BY 1 DO
		(* - Dtermination de la destination de la pompe, si la cuve est compte *)
		IF (Conf_Adj_dest_cuves[Idx_P1] &gt;= 1)
		AND (Conf_Adj_dest_cuves[Idx_P1] &lt;= 20) THEN (* Cuve Adj compte *)
			Dest_ppe_Adj := Conf_ppe_dest[Conf_Adj_dest_cuves[Idx_P1]];
		ELSE
			Dest_ppe_Adj := 0;
		END_IF;

		Cde_dosage_Adj[Idx_P1] := 
			NOT Info_Plant_In_Auto
			AND Pup_Cde_dos_Adj[Idx_P1]
			AND NOT Tble_Alarme_Niv_haut_ADJ_Comp[No_Comp_ADJ[Idx_P1]]
			AND ((
				(Conf_Adj_dest_cuves[Idx_P1] = 0) (* Cuve Adj pese *)
				AND (Numero_manu_pup_Adj = idx_P1)
				AND Info_balance_Adj_fermee (* AND NOT Vidange_basc_adj_PPE) (*Cas ou l'on  pas de capteur*) *)
				AND NOT Alarme_poids_max_Adj
				AND NOT Alarme_Com_HMS_Profibus_Adj
			) OR (
				(Dest_ppe_Adj = 13) (* Cuve Adj compte, destination bascule Eau1 *)
				AND (Numero_manu_pup_Eau1 = 0) (* Pas de cuve pese en cours de dosage vers la bascule Eau1 *)
				AND Info_balance_Eau1_fermee
				AND NOT Alarme_poids_max_Eau1
				AND NOT Alarme_Com_HMS_Profibus_Eau1
			) OR (
				(Conf_Adj_dest_cuves[Idx_P1] &lt;&gt; 0) (* Cuve Adj compte *)
				AND (Dest_ppe_Adj &lt;&gt; 13) (* ...mais pas vers Eau1 *)
			))
			AND NOT Alarme_Com_API_ModbusTCP_1 (* TOUTES BASCULES SUR HMS ... *)
			AND NOT Alarme_Controle_Tension
			AND NOT Alarme_prot_mot_Dos_ADJ;   (* Modif Inde *)
	END_FOR;


	(********************** ADDITIONNELLE ****************************)

	(* "Numero_manu_pup_*" contient le numero de l'organe de stockage surlequel *)
	(* un dosage est autoris. On n'autorise qu'un dosage par bascule pour pouvoir *)
	(* dterminer les consommations par organe de stockage. *)
	(* On n'autorise le changement d'organe ou la vidange de la bascule qu'une fois *)
	(* le cumul fait. Dans ce but, on passe "Numero_manu_pup_*"  0 pour savoir que *)
	(* le cumul est fait. *)
	FOR idx_P1:=1 TO Conf_basc_add_nb_organes BY 1 DO
		IF (Pup_Cde_dos_Add[idx_P1] = TRUE) THEN (* Demande de dosage par le pupitre *)
			IF (Numero_manu_pup_Add = 0) THEN (* Basc Add dispo pour un dosage (le cumul conso a t mis  jour aprs le dosage prcdent) *)
				Numero_manu_pup_Add := idx_P1;
			END_IF;
		END_IF;
	END_FOR;

	(* FAIRE LE DOSAGE *)
