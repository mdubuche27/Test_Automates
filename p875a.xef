	(* aussi quand c'est la 1re gche de la fabrication *)
	LDN CGG_activee
	OR (TG_IN[NG_Gra2].Proprietes_gachee
		AND 16#0002 (* 1re gche de la fabrication *)
		GT 0
	)
	AND (NP_Gra2
		EQ 1 (* 1er produit de la gche *)
	)
	JMPCN init_corr_g_g_gra2__fin
