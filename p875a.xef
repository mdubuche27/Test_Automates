		LD CGG_activee_Anh
		ST Flags_pesage_Anh.CGG_ON
 
 		(* Dernire gache *)
 		LD TG_IN[NG_Anh].Proprietes_gachee
 		AND 16#0001
 		NE 0
 		ST Flags_pesage_Anh.Derniere_gachee

 		(* Premire gache *)
 		LD TG_IN[NG_Anh].Proprietes_gachee
 		AND 16#0002
