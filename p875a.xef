(* par pompe  destination de la bascule Eau1 pour autoriser le dosage d'une *)
(* cuve pese en manuel logiciel, pour ne pas que le cumul de la bascule Eau1 *)
(* soit fauss. *)
(* On utilisera la sortie de la temporisation pour autoriser un dosage. *)
(* - Dtecter s'il y a un dosage compt vers Eau1 *)
LD TRUE
R Detect_pompe_vers_Eau1 (* Init *)
LD 0
ST idx_ppe (* Init *)
detect_pompe_vers_eau1_1:
	LD idx_ppe
	ADD 1
	ST idx_ppe
	GT Conf_ppe_nb_ppes
	JMPC detect_pompe_vers_eau1__fin
		LD Conf_ppe_dest[idx_ppe]
		EQ 13
		JMPCN detect_pompe_vers_eau1_1
		(* La pompe est  destination de la bascule Eau1 *)
		LD TRUE
		AND (Conf_Adj_origine_pompes[idx_ppe]
			GE 1
		)
		AND (Conf_Adj_origine_pompes[idx_ppe]
			LT 20
		)
		JMPCN detect_pompe_vers_eau1_1
		(* La pompe est associe  une cuve *)
		LD Cde_dosage_Ppe[idx_ppe]
		OR Cde_dosage_Adj[Conf_Adj_origine_pompes[idx_ppe]] (* Note : en manu pupitre, on ne passe pas forcment par "Cde_dosage_Ppe[]" *)
		S Detect_pompe_vers_Eau1 (* La pompe est commande *)
		JMPC detect_pompe_vers_eau1__fin (* Inutile de parcourir les autres pompes *)
	JMP detect_pompe_vers_eau1_1 (* Pompe suivante *)
detect_pompe_vers_eau1__fin:
(* - Attendre l'aprs coulant de la pompe et la stabilisation de la bascule *)
LDN Detect_pompe_vers_Eau1
